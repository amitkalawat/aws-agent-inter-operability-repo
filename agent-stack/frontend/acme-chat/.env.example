# ACME Chat Frontend Configuration
# Copy this file to .env.local and fill in your values

# AWS Region
REACT_APP_AWS_REGION=us-west-2

# Cognito Configuration (from CDK deployment outputs)
REACT_APP_COGNITO_USER_POOL_ID=us-west-2_XXXXXXXXX
REACT_APP_COGNITO_APP_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# AgentCore Configuration (from CDK deployment outputs)
REACT_APP_AGENTCORE_ARN=arn:aws:bedrock-agentcore:us-west-2:XXXXXXXXXXXX:runtime/your-agent-id

# Demo User (optional - for testing)
REACT_APP_DEMO_USERNAME=admin@acme.com
REACT_APP_DEMO_PASSWORD=YourSecurePassword123!
